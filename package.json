    "react-apollo-hooks": "^0.5.0",
    "react-native": "0.59.1",
    "react-native-actionsheet": "^2.4.2",
    "react-native-elements": "^1.0.0-beta7",
    "react-native-fade-in-image": "^1.4.1",
    "react-native-google-signin": "^1.0.2",
    "react-native-image-zoom-viewer": "^2.2.18",
